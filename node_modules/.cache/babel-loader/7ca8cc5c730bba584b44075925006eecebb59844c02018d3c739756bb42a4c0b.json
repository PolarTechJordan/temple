{"ast":null,"code":"import { useRouter } from 'vue-router';\nimport { useAppStore } from '@/stores/app';\nimport { computed } from 'vue';\nexport default {\n  name: 'MyMeritPage',\n  setup() {\n    const router = useRouter();\n    const appStore = useAppStore();\n    const currentLevelInfo = computed(() => appStore.currentLevelInfo);\n    const nextLevelInfo = computed(() => appStore.nextLevelInfo);\n    const progressPercentage = computed(() => appStore.levelProgress);\n    return {\n      router,\n      appStore,\n      currentLevelInfo,\n      nextLevelInfo,\n      progressPercentage\n    };\n  },\n  data() {\n    return {\n      levelIcon: '/images/level1.png'\n    };\n  },\n  computed: {\n    levelDisplayName() {\n      const levelNames = {\n        beginner: '初学者',\n        devotee: '虔诚者',\n        believer: '信徒',\n        enlightened: '觉悟者',\n        sage: '圣贤'\n      };\n      return levelNames[this.currentLevelInfo.name] || '未知等级';\n    },\n    progressCurrent() {\n      if (!this.nextLevelInfo) return 0;\n      return this.appStore.meritPoints - this.currentLevelInfo.minPoints;\n    },\n    progressNeeded() {\n      if (!this.nextLevelInfo) return 0;\n      return this.nextLevelInfo.minPoints - this.currentLevelInfo.minPoints;\n    }\n  },\n  methods: {\n    goBack() {\n      this.router.go(-1);\n    }\n  }\n};","map":{"version":3,"names":["useRouter","useAppStore","computed","name","setup","router","appStore","currentLevelInfo","nextLevelInfo","progressPercentage","levelProgress","data","levelIcon","levelDisplayName","levelNames","beginner","devotee","believer","enlightened","sage","progressCurrent","meritPoints","minPoints","progressNeeded","methods","goBack","go"],"sources":["/Users/Jordan/workSpace/temple/src/views/MyMeritPage.vue"],"sourcesContent":["<template>\n  <div class=\"my-merit-page\">\n    <div class=\"container\">\n      <h1>我的功德</h1>\n      \n      <div class=\"merit-container\">\n        <!-- 功德概览 -->\n        <div class=\"merit-overview\">\n          <div class=\"current-merit\">\n            <h2>当前功德</h2>\n            <div class=\"merit-display\">\n              <div class=\"score\">{{ appStore.meritPoints }}</div>\n              <div class=\"unit\">功德分</div>\n            </div>\n          </div>\n\n          <div class=\"level-info\">\n            <h3>等级信息</h3>\n            <div class=\"level-display\">\n              <div class=\"level-badge\">\n                <img :src=\"levelIcon\" :alt=\"currentLevelInfo.name\" class=\"level-icon\" />\n                <span class=\"level-name\">{{ levelDisplayName }}</span>\n              </div>\n              <div class=\"progress-section\">\n                <div class=\"progress-bar\">\n                  <div class=\"progress-fill\" :style=\"{ width: progressPercentage + '%' }\"></div>\n                </div>\n                <div class=\"progress-text\">\n                  <span>{{ currentLevelInfo.minPoints }}</span>\n                  <span>{{ nextLevelInfo ? `${progressCurrent}/${progressNeeded}` : '已达最高级' }}</span>\n                  <span>{{ nextLevelInfo ? nextLevelInfo.minPoints : '∞' }}</span>\n                </div>\n              </div>\n            </div>\n          </div>\n        </div>\n\n        <!-- 返回按钮 -->\n        <div class=\"action-buttons\">\n          <button @click=\"goBack\" class=\"back-btn\">返回</button>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport { useRouter } from 'vue-router'\nimport { useAppStore } from '@/stores/app'\nimport { computed } from 'vue'\n\nexport default {\n  name: 'MyMeritPage',\n  setup() {\n    const router = useRouter()\n    const appStore = useAppStore()\n    \n    const currentLevelInfo = computed(() => appStore.currentLevelInfo)\n    const nextLevelInfo = computed(() => appStore.nextLevelInfo)\n    const progressPercentage = computed(() => appStore.levelProgress)\n    \n    return {\n      router,\n      appStore,\n      currentLevelInfo,\n      nextLevelInfo,\n      progressPercentage\n    }\n  },\n  data() {\n    return {\n      levelIcon: '/images/level1.png'\n    }\n  },\n  computed: {\n    levelDisplayName() {\n      const levelNames = {\n        beginner: '初学者',\n        devotee: '虔诚者',\n        believer: '信徒',\n        enlightened: '觉悟者',\n        sage: '圣贤'\n      }\n      return levelNames[this.currentLevelInfo.name] || '未知等级'\n    },\n    progressCurrent() {\n      if (!this.nextLevelInfo) return 0\n      return this.appStore.meritPoints - this.currentLevelInfo.minPoints\n    },\n    progressNeeded() {\n      if (!this.nextLevelInfo) return 0\n      return this.nextLevelInfo.minPoints - this.currentLevelInfo.minPoints\n    }\n  },\n  methods: {\n    goBack() {\n      this.router.go(-1)\n    }\n  }\n}\n</script>\n\n<style scoped>\n.my-merit-page {\n  min-height: 100vh;\n  background: linear-gradient(135deg, #667eea 0%, #764ba2 100%);\n  padding: 2rem;\n}\n\n.container {\n  max-width: 800px;\n  margin: 0 auto;\n}\n\nh1 {\n  text-align: center;\n  color: white;\n  margin-bottom: 2rem;\n  font-size: 2.5rem;\n}\n\n.merit-container {\n  background: rgba(255, 255, 255, 0.9);\n  border-radius: 20px;\n  padding: 2rem;\n  backdrop-filter: blur(10px);\n}\n\n.merit-overview {\n  display: grid;\n  grid-template-columns: 1fr 2fr;\n  gap: 2rem;\n  margin-bottom: 2rem;\n  padding: 1.5rem;\n  background: linear-gradient(135deg, #f8f9fa 0%, #e9ecef 100%);\n  border-radius: 15px;\n}\n\n.current-merit {\n  text-align: center;\n}\n\n.current-merit h2 {\n  color: #333;\n  margin-bottom: 1rem;\n}\n\n.merit-display {\n  display: flex;\n  flex-direction: column;\n  align-items: center;\n}\n\n.score {\n  font-size: 3rem;\n  font-weight: bold;\n  color: #667eea;\n  line-height: 1;\n}\n\n.unit {\n  color: #666;\n  font-size: 1rem;\n  margin-top: 0.5rem;\n}\n\n.level-info h3 {\n  color: #333;\n  margin-bottom: 1rem;\n}\n\n.level-display {\n  display: flex;\n  flex-direction: column;\n  gap: 1rem;\n}\n\n.level-badge {\n  display: flex;\n  align-items: center;\n  gap: 1rem;\n}\n\n.level-icon {\n  width: 48px;\n  height: 48px;\n}\n\n.level-name {\n  font-size: 1.2rem;\n  font-weight: 600;\n  color: #333;\n}\n\n.progress-section {\n  flex: 1;\n}\n\n.progress-bar {\n  width: 100%;\n  height: 12px;\n  background: #e9ecef;\n  border-radius: 6px;\n  overflow: hidden;\n  margin-bottom: 0.5rem;\n}\n\n.progress-fill {\n  height: 100%;\n  background: linear-gradient(90deg, #667eea 0%, #764ba2 100%);\n  transition: width 0.3s ease;\n}\n\n.progress-text {\n  display: flex;\n  justify-content: space-between;\n  font-size: 0.9rem;\n  color: #666;\n}\n\n.action-buttons {\n  display: flex;\n  justify-content: center;\n}\n\n.back-btn {\n  padding: 0.8rem 2rem;\n  border: none;\n  border-radius: 25px;\n  font-size: 1rem;\n  font-weight: 600;\n  cursor: pointer;\n  transition: all 0.3s ease;\n  background: #6c757d;\n  color: white;\n}\n\n.back-btn:hover {\n  transform: translateY(-2px);\n  box-shadow: 0 5px 15px rgba(0, 0, 0, 0.2);\n}\n\n@media (max-width: 768px) {\n  .merit-overview {\n    grid-template-columns: 1fr;\n    gap: 1rem;\n  }\n}\n</style> "],"mappings":"AA+CA,SAASA,SAAQ,QAAS,YAAW;AACrC,SAASC,WAAU,QAAS,cAAa;AACzC,SAASC,QAAO,QAAS,KAAI;AAE7B,eAAe;EACbC,IAAI,EAAE,aAAa;EACnBC,KAAKA,CAAA,EAAG;IACN,MAAMC,MAAK,GAAIL,SAAS,CAAC;IACzB,MAAMM,QAAO,GAAIL,WAAW,CAAC;IAE7B,MAAMM,gBAAe,GAAIL,QAAQ,CAAC,MAAMI,QAAQ,CAACC,gBAAgB;IACjE,MAAMC,aAAY,GAAIN,QAAQ,CAAC,MAAMI,QAAQ,CAACE,aAAa;IAC3D,MAAMC,kBAAiB,GAAIP,QAAQ,CAAC,MAAMI,QAAQ,CAACI,aAAa;IAEhE,OAAO;MACLL,MAAM;MACNC,QAAQ;MACRC,gBAAgB;MAChBC,aAAa;MACbC;IACF;EACF,CAAC;EACDE,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,SAAS,EAAE;IACb;EACF,CAAC;EACDV,QAAQ,EAAE;IACRW,gBAAgBA,CAAA,EAAG;MACjB,MAAMC,UAAS,GAAI;QACjBC,QAAQ,EAAE,KAAK;QACfC,OAAO,EAAE,KAAK;QACdC,QAAQ,EAAE,IAAI;QACdC,WAAW,EAAE,KAAK;QAClBC,IAAI,EAAE;MACR;MACA,OAAOL,UAAU,CAAC,IAAI,CAACP,gBAAgB,CAACJ,IAAI,KAAK,MAAK;IACxD,CAAC;IACDiB,eAAeA,CAAA,EAAG;MAChB,IAAI,CAAC,IAAI,CAACZ,aAAa,EAAE,OAAO;MAChC,OAAO,IAAI,CAACF,QAAQ,CAACe,WAAU,GAAI,IAAI,CAACd,gBAAgB