{"ast":null,"code":"export default {\n  name: 'WorshipPage',\n  data() {\n    return {\n      worshipVideoSrc: '/videos/worship.mp4',\n      worshipMessage: '虔诚上香',\n      worshipDescription: '正在为您上香祈福...',\n      showContinueButton: false\n    };\n  },\n  methods: {\n    onVideoEnded() {\n      this.worshipMessage = '上香完成';\n      this.worshipDescription = '神明已收到您的祈福，功德+1';\n      this.showContinueButton = true;\n    },\n    continueToMerit() {\n      this.$router.push('/merit');\n    }\n  }\n};","map":{"version":3,"names":["name","data","worshipVideoSrc","worshipMessage","worshipDescription","showContinueButton","methods","onVideoEnded","continueToMerit","$router","push"],"sources":["/Users/Jordan/workSpace/temple/src/views/WorshipPage.vue"],"sourcesContent":["<template>\n  <div class=\"worship-page\">\n    <div class=\"container\">\n      <h1>{{ $t('worship.title') }}</h1>\n      <div class=\"worship-container\">\n        <div class=\"worship-video\">\n          <video\n            ref=\"worshipVideo\"\n            :src=\"worshipVideoSrc\"\n            autoplay\n            muted\n            @ended=\"onVideoEnded\"\n            class=\"video-player\"\n          ></video>\n        </div>\n        <div class=\"worship-message\">\n          <h2>{{ worshipMessage }}</h2>\n          <p>{{ worshipDescription }}</p>\n        </div>\n        <div v-if=\"showContinueButton\" class=\"continue-section\">\n          <button @click=\"continueToMerit\" class=\"continue-btn\">\n            查看功德\n          </button>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nexport default {\n  name: 'WorshipPage',\n  data() {\n    return {\n      worshipVideoSrc: '/videos/worship.mp4',\n      worshipMessage: '虔诚上香',\n      worshipDescription: '正在为您上香祈福...',\n      showContinueButton: false\n    }\n  },\n  methods: {\n    onVideoEnded() {\n      this.worshipMessage = '上香完成'\n      this.worshipDescription = '神明已收到您的祈福，功德+1'\n      this.showContinueButton = true