{"ast":null,"code":"import { useI18n } from 'vue-i18n';\nimport { useRouter } from 'vue-router';\nimport { useAppStore } from '@/stores/app';\nimport { computed } from 'vue';\nexport default {\n  name: 'InputPage',\n  setup() {\n    const {\n      t,\n      locale\n    } = useI18n();\n    const router = useRouter();\n    const appStore = useAppStore();\n\n    // ÂΩìÂâçËØ≠Ë®Ä\n    const currentLocale = computed(() => locale.value);\n\n    // ÂàáÊç¢ËØ≠Ë®Ä\n    const switchLanguage = lang => {\n      locale.value = lang;\n      // Ê†πÊçÆËØ≠Ë®ÄÂàáÊç¢Ë∑ØÁî±\n      const currentRoute = router.currentRoute.value;\n      if (lang === 'en-US') {\n        // ÂàáÊç¢Âà∞Ëã±ÊñáË∑ØÁî±\n        if (!currentRoute.path.startsWith('/en')) {\n          router.push('/en' + currentRoute.path);\n        }\n      } else {\n        // ÂàáÊç¢Âà∞‰∏≠ÊñáË∑ØÁî±\n        if (currentRoute.path.startsWith('/en')) {\n          router.push(currentRoute.path.replace('/en', ''));\n        }\n      }\n    };\n\n    // ËØ≠Ë®ÄÊÑüÁü•ÁöÑÂØºËà™ÂáΩÊï∞\n    const navigateTo = path => {\n      const currentPath = router.currentRoute.value.path;\n      const isEnglish = currentPath.startsWith('/en');\n      const targetPath = isEnglish ? `/en${path}` : path;\n      router.push(targetPath);\n    };\n\n    // ÁÆÄÂåñÁöÑÈí±ÂåÖÂú∞ÂùÄÊòæÁ§∫\n    const shortAddress = computed(() => {\n      if (appStore.walletAddress) {\n        return appStore.walletAddress.slice(0, 6) + '...' + appStore.walletAddress.slice(-4);\n      }\n      return '';\n    });\n    return {\n      t,\n      router,\n      appStore,\n      navigateTo,\n      currentLocale,\n      switchLanguage,\n      shortAddress\n    };\n  },\n  data() {\n    return {\n      wishText: '',\n      connecting: false,\n      error: null\n    };\n  },\n  methods: {\n    async connectWallet() {\n      this.connecting = true;\n      this.error = null;\n      try {\n        // Ê®°ÊãüOKXÈí±ÂåÖËøûÊé•\n        await new Promise(resolve => setTimeout(resolve, 2000));\n\n        // ËøôÈáåÂ∫îËØ•ÂÆûÈôÖËøûÊé•OKXÈí±ÂåÖ\n        console.log('ËøûÊé•OKXÈí±ÂåÖ');\n\n        // ‰ΩøÁî® Pinia store ‰øùÂ≠òËøûÊé•Áä∂ÊÄÅ\n        const mockAddress = '0x' + Math.random().toString(16).substr(2, 40);\n        this.appStore.connectWallet(mockAddress, 'OKX');\n      } catch (error) {\n        this.error = 'ËøûÊé•Â§±Ë¥•ÔºåËØ∑ÈáçËØï';\n        console.error('Èí±ÂåÖËøûÊé•Â§±Ë¥•:', error);\n      } finally {\n        this.connecting = false;\n      }\n    },\n    disconnectWallet() {\n      this.appStore.disconnectWallet();\n    },\n    submitWish() {\n      if (this.wishText.trim() && this.appStore.isWalletConnected) {\n        // ‰øùÂ≠òËÆ∏ÊÑøÂÜÖÂÆπÂà∞store\n        this.appStore.saveUserWish(this.wishText);\n        // Áõ¥Êé•Ë∑≥ËΩ¨Âà∞ÁÆóÂëΩÈ°µÈù¢Ôºà‰∏çÂÜçÈúÄË¶ÅloginÈ°µÈù¢Ôºâ\n        this.navigateTo('/calculate');\n      }\n    }\n  }\n};","map":{"version":3,"names":["useI18n","useRouter","useAppStore","computed","name","setup","t","locale","router","appStore","currentLocale","value","switchLanguage","lang","currentRoute","path","startsWith","push","replace","navigateTo","currentPath","isEnglish","targetPath","shortAddress","walletAddress","slice","data","wishText","connecting","error","methods","connectWallet","Promise","resolve","setTimeout","console","log","mockAddress","Math","random","toString","substr","disconnectWallet","submitWish","trim","isWalletConnected","saveUserWish"],"sources":["/Users/Jordan/workSpace/temple/src/views/InputPage.vue"],"sourcesContent":["<template>\n  <div class=\"input-page\">\n    <!-- ËØ≠Ë®ÄÂàáÊç¢ÊåâÈíÆÔºàÂè™Âú®Ëøô‰∏™È°µÈù¢ÊòæÁ§∫Ôºâ -->\n    <div class=\"language-switcher\">\n      <button \n        @click=\"switchLanguage('zh-CN')\"\n        :class=\"{ active: currentLocale === 'zh-CN' }\"\n      >\n        ‰∏≠Êñá\n      </button>\n      <button \n        @click=\"switchLanguage('en-US')\"\n        :class=\"{ active: currentLocale === 'en-US' }\"\n      >\n        English\n      </button>\n    </div>\n\n    <!-- Âè≥‰∏äËßíÈí±ÂåÖËøûÊé•ÊåâÈíÆ -->\n    <div class=\"wallet-header\">\n      <button \n        v-if=\"!appStore.isWalletConnected\"\n        @click=\"showWalletModal = true\"\n        class=\"header-connect-btn\"\n      >\n        Connect\n      </button>\n      <div v-else class=\"wallet-status\">\n        <span class=\"wallet-address-short\">{{ shortAddress }}</span>\n        <button @click=\"disconnectWallet\" class=\"disconnect-btn-small\">√ó</button>\n      </div>\n    </div>\n\n    <div class=\"container\">\n      <h1>{{ $t('input.title') }}</h1>\n      \n      <div class=\"main-content\">\n        <!-- Â∑¶‰æßÔºöËÆ∏ÊÑøÊ±† -->\n        <div class=\"wish-section\">\n          <div class=\"wish-pool\">\n            <div class=\"pool-container\">\n              <textarea\n                v-model=\"wishText\"\n                :placeholder=\"$t('input.placeholder')\"\n                class=\"wish-input\"\n                rows=\"6\"\n                maxlength=\"200\"\n              ></textarea>\n              <div class=\"char-counter\">{{ wishText.length }}/200</div>\n            </div>\n            <button \n              @click=\"submitWish\"\n              :disabled=\"!wishText.trim() || !appStore.isWalletConnected\"\n              class=\"submit-btn\"\n            >\n              {{ $t('input.submit') }}\n            </button>\n            <div v-if=\"!appStore.isWalletConnected\" class=\"wallet-warning\">\n              ËØ∑ÂÖàËøûÊé•Èí±ÂåÖÂÜçÊèê‰∫§ËÆ∏ÊÑø\n            </div>\n          </div>\n        </div>\n\n        <!-- Âè≥‰æßÔºöÈí±ÂåÖËøûÊé• -->\n        <div class=\"wallet-section\">\n          <div v-if=\"!appStore.isWalletConnected\" class=\"wallet-connect\">\n            <h2>ËøûÊé•Èí±ÂåÖ</h2>\n            <p class=\"connect-description\">ËøûÊé•ÊÇ®ÁöÑÈí±ÂåÖÂºÄÂßãÁ•àÁ¶è‰πãÊóÖ</p>\n            \n            <!-- ËøûÊé•ÊåâÈíÆ -->\n            <button \n              @click=\"connectWallet\"\n              :disabled=\"connecting\"\n              class=\"connect-btn\"\n            >\n              {{ connecting ? 'ËøûÊé•‰∏≠...' : 'Connect' }}\n            </button>\n            \n            <!-- OKX Èí±ÂåÖÈÄâÈ°π -->\n            <button \n              @click=\"connectWallet\"\n              :disabled=\"connecting\"\n              class=\"wallet-option-btn\"\n              :class=\"{ 'connecting': connecting }\"\n            >\n              <!-- NEW Ê†áÁ≠æ -->\n              <div class=\"new-badge\">NEW!</div>\n              \n              <!-- Èí±ÂåÖÂõæÊ†á -->\n              <div class=\"wallet-icon-container\">\n                <div class=\"wallet-icon-bg\">\n                  üè∫\n                </div>\n              </div>\n              \n              <!-- Èí±ÂåÖ‰ø°ÊÅØ -->\n              <div class=\"wallet-info-modern\">\n                <p class=\"wallet-title\">OKX Ë¥¢Á•ûÈí±ÂåÖ</p>\n                <p class=\"wallet-desc\">Á•àÁ¶è‰∏ìÁî®ÔºåÂÆâÂÖ®ÂèØÈù†ÔºÅ</p>\n              </div>\n              \n              <!-- Ë£ÖÈ•∞ÊÄßSVGËÉåÊôØ -->\n              <svg xmlns=\"http://www.w3.org/2000/svg\" viewBox=\"0 0 303 54\" fill=\"none\" class=\"wallet-bg-svg\">\n                <defs>\n                  <linearGradient id=\"walletGradient\" x1=\"0%\" y1=\"0%\" x2=\"100%\" y2=\"0%\">\n                    <stop offset=\"0%\" style=\"stop-color:#667eea;stop-opacity:0.1\" />\n                    <stop offset=\"50%\" style=\"stop-color:#764ba2;stop-opacity:0.2\" />\n                    <stop offset=\"100%\" style=\"stop-color:#667eea;stop-opacity:0.1\" />\n                  </linearGradient>\n                </defs>\n                <path d=\"M0 27C50 10, 100 44, 150 27C200 10, 250 44, 303 27V54H0V27Z\" fill=\"url(#walletGradient)\"/>\n              </svg>\n            </button>\n\n            <div v-if=\"error\" class=\"error-message\">\n              {{ error }}\n            </div>\n          </div>\n\n          <div v-else class=\"wallet-connected\">\n            <h2>Èí±ÂåÖÂ∑≤ËøûÊé•</h2>\n            <div class=\"connected-info\">\n              <div class=\"wallet-address\">\n                <span class=\"label\">Âú∞ÂùÄ:</span>\n                <span class=\"address\">{{ shortAddress }}</span>\n              </div>\n              <div class=\"wallet-type\">\n                <span class=\"label\">Èí±ÂåÖ:</span>\n                <span class=\"type\">{{ appStore.walletType }}</span>\n              </div>\n            </div>\n            <button @click=\"disconnectWallet\" class=\"disconnect-btn\">\n              Êñ≠ÂºÄËøûÊé•\n            </button>\n          </div>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport { useI18n } from 'vue-i18n'\nimport { useRouter } from 'vue-router'\nimport { useAppStore } from '@/stores/app'\nimport { computed } from 'vue'\n\nexport default {\n  name: 'InputPage',\n  setup() {\n    const { t, locale } = useI18n()\n    const router = useRouter()\n    const appStore = useAppStore()\n\n    // ÂΩìÂâçËØ≠Ë®Ä\n    const currentLocale = computed(() => locale.value)\n\n    // ÂàáÊç¢ËØ≠Ë®Ä\n    const switchLanguage = (lang) => {\n      locale.value = lang\n      // Ê†πÊçÆËØ≠Ë®ÄÂàáÊç¢Ë∑ØÁî±\n      const currentRoute = router.currentRoute.value\n      if (lang === 'en-US') {\n        // ÂàáÊç¢Âà∞Ëã±ÊñáË∑ØÁî±\n        if (!currentRoute.path.startsWith('/en')) {\n          router.push('/en' + currentRoute.path)\n        }\n      } else {\n        // ÂàáÊç¢Âà∞‰∏≠ÊñáË∑ØÁî±\n        if (currentRoute.path.startsWith('/en')) {\n          router.push(currentRoute.path.replace('/en', ''))\n        }\n      }\n    }\n\n    // ËØ≠Ë®ÄÊÑüÁü•ÁöÑÂØºËà™ÂáΩÊï∞\n    const navigateTo = (path) => {\n      const currentPath = router.currentRoute.value.path\n      const isEnglish = currentPath.startsWith('/en')\n      const targetPath = isEnglish ? `/en${path}` : path\n      router.push(targetPath)\n    }\n\n    // ÁÆÄÂåñÁöÑÈí±ÂåÖÂú∞ÂùÄÊòæÁ§∫\n    const shortAddress = computed(() => {\n      if (appStore.walletAddress) {\n        return appStore.walletAddress.slice(0, 6) + '...' + appStore.walletAddress.slice(-4)\n      }\n      return ''\n    })\n\n    return {\n      t,\n      router,\n      appStore,\n      navigateTo,\n      currentLocale,\n      switchLanguage,\n      shortAddress\n    }\n  },\n  data() {\n    return {\n      wishText: '',\n      connecting: false,\n      error: null\n    }\n  },\n  methods: {\n    async connectWallet() {\n      this.connecting = true\n      this.error = null\n      \n      try {\n        // Ê®°ÊãüOKXÈí±ÂåÖËøûÊé•\n        await new Promise(resolve => setTimeout(resolve, 2000))\n        \n        // ËøôÈáåÂ∫îËØ•ÂÆûÈôÖËøûÊé•OKXÈí±ÂåÖ\n        console.log('ËøûÊé•OKXÈí±ÂåÖ')\n        \n        // ‰ΩøÁî® Pinia store ‰øùÂ≠òËøûÊé•Áä∂ÊÄÅ\n        const mockAddress = '0x' + Math.random().toString(16).substr(2, 40)\n        this.appStore.connectWallet(mockAddress, 'OKX')\n        \n      } catch (error) {\n        this.error = 'ËøûÊé•Â§±Ë¥•ÔºåËØ∑ÈáçËØï'\n        console.error('Èí±ÂåÖËøûÊé•Â§±Ë¥•:', error)\n      } finally {\n        this.connecting = false\n      }\n    },\n    \n    disconnectWallet() {\n      this.appStore.disconnectWallet()\n    },\n    \n    submitWish() {\n      if (this.wishText.trim() && this.appStore.isWalletConnected) {\n        // ‰øùÂ≠òËÆ∏ÊÑøÂÜÖÂÆπÂà∞store\n        this.appStore.saveUserWish(this.wishText)\n        // Áõ¥Êé•Ë∑≥ËΩ¨Âà∞ÁÆóÂëΩÈ°µÈù¢Ôºà‰∏çÂÜçÈúÄË¶ÅloginÈ°µÈù¢Ôºâ\n        this.navigateTo('/calculate')\n      }\n    }\n  }\n}\n</script>\n\n<style scoped>\n.input-page {\n  min-height: 100vh;\n  background: linear-gradient(135deg, #667eea 0%, #764ba2 100%);\n  padding: 2rem;\n  position: relative;\n}\n\n.language-switcher {\n  position: fixed;\n  top: 20px;\n  left: 20px;\n  z-index: 1000;\n  display: flex;\n  gap: 10px;\n\n  button {\n    padding: 8px 16px;\n    border: 1px solid #ddd;\n    background: rgba(255, 255, 255, 0.9);\n    border-radius: 4px;\n    cursor: pointer;\n    transition: all 0.3s;\n    backdrop-filter: blur(10px);\n\n    &:hover {\n      background: rgba(255, 255, 255, 1);\n    }\n\n    &.active {\n      background: #007bff;\n      color: white;\n      border-color: #007bff;\n    }\n  }\n}\n\n.container {\n  max-width: 1200px;\n  margin: 0 auto;\n}\n\nh1 {\n  text-align: center;\n  color: white;\n  margin-bottom: 2rem;\n  font-size: 2.5rem;\n}\n\n.main-content {\n  display: grid;\n  grid-template-columns: 1fr 1fr;\n  gap: 2rem;\n  align-items: start;\n}\n\n.wish-section {\n  background: rgba(255, 255, 255, 0.9);\n  border-radius: 20px;\n  padding: 2rem;\n  backdrop-filter: blur(10px);\n}\n\n.wish-pool {\n  width: 100%;\n}\n\n.pool-container {\n  position: relative;\n  margin-bottom: 1.5rem;\n}\n\n.wish-input {\n  width: 100%;\n  border: 2px solid #ddd;\n  border-radius: 10px;\n  padding: 1rem;\n  font-size: 1.1rem;\n  resize: vertical;\n  min-height: 120px;\n  font-family: inherit;\n}\n\n.wish-input:focus {\n  outline: none;\n  border-color: #667eea;\n}\n\n.char-counter {\n  position: absolute;\n  bottom: 10px;\n  right: 15px;\n  color: #888;\n  font-size: 0.9rem;\n}\n\n.submit-btn {\n  width: 100%;\n  padding: 1rem;\n  background: linear-gradient(135deg, #667eea 0%, #764ba2 100%);\n  color: white;\n  border: none;\n  border-radius: 10px;\n  font-size: 1.2rem;\n  font-weight: 600;\n  cursor: pointer;\n  transition: all 0.3s ease;\n}\n\n.submit-btn:hover:not(:disabled) {\n  transform: translateY(-2px);\n  box-shadow: 0 5px 15px rgba(102, 126, 234, 0.4);\n}\n\n.submit-btn:disabled {\n  opacity: 0.6;\n  cursor: not-allowed;\n}\n\n.wallet-warning {\n  margin-top: 1rem;\n  padding: 0.8rem;\n  background: #fff3cd;\n  color: #856404;\n  border-radius: 8px;\n  text-align: center;\n  font-size: 0.9rem;\n}\n\n.wallet-section {\n  background: rgba(255, 255, 255, 0.9);\n  border-radius: 20px;\n  padding: 2rem;\n  backdrop-filter: blur(10px);\n}\n\n.wallet-connect h2,\n.wallet-connected h2 {\n  color: #333;\n  margin-bottom: 1rem;\n  text-align: center;\n}\n\n.connect-description {\n  text-align: center;\n  color: #666;\n  margin-bottom: 2rem;\n}\n\n/* ËøûÊé•ÊåâÈíÆÊ†∑Âºè */\n.connect-btn {\n  height: 2rem;\n  border-radius: 9999px;\n  border: 1px solid #171717;\n  background: #171717;\n  padding: 0 0.625rem;\n  font-size: 0.75rem;\n  font-weight: 600;\n  color: #667eea;\n  cursor: pointer;\n  transition: all 0.3s ease;\n  margin-bottom: 1rem;\n  width: 100%;\n  \n  &:hover:not(:disabled) {\n    border-color: #667eea;\n    background: #0a0a0a;\n  }\n  \n  &:disabled {\n    opacity: 0.6;\n    cursor: not-allowed;\n  }\n}\n\n@media (min-width: 768px) {\n  .connect-btn {\n    height: 2.25rem;\n  }\n}\n\n/* Èí±ÂåÖÈÄâÈ°πÊåâÈíÆÊ†∑Âºè */\n.wallet-option-btn {\n  position: relative;\n  display: flex;\n  align-items: center;\n  justify-content: flex-start;\n  border-radius: 0.75rem;\n  border: 1px solid #262626;\n  background: #171717;\n  font-size: 0.75rem;\n  height: 6rem;\n  width: 100%;\n  gap: 1rem;\n  padding: 1rem;\n  cursor: pointer;\n  transition: all 0.3s ease;\n  color: #e5e5e5;\n  overflow: hidden;\n  \n  &:hover:not(:disabled) {\n    border-color: #404040;\n    background: #1a1a1a;\n  }\n  \n  &:disabled {\n    opacity: 0.6;\n    cursor: not-allowed;\n  }\n}\n\n@media (min-width: 768px) {\n  .wallet-option-btn {\n    font-size: 0.875rem;\n  }\n}\n\n/* NEW ÂæΩÁ´† */\n.new-badge {\n  position: absolute;\n  top: -0.375rem;\n  right: 1rem;\n  border-radius: 0.25rem;\n  background: #667eea;\n  padding: 0.125rem 0.25rem;\n  font-size: 0.75rem;\n  font-weight: 700;\n  color: rgba(7, 9, 15, 0.75);\n  z-index: 10;\n}\n\n/* Èí±ÂåÖÂõæÊ†áÂÆπÂô® */\n.wallet-icon-container {\n  display: flex;\n  width: 2.625rem;\n  height: 2.625rem;\n  align-items: center;\n  justify-content: center;\n  flex-shrink: 0;\n}\n\n.wallet-icon-bg {\n  width: 2rem;\n  height: 2rem;\n  border-radius: 0.5rem;\n  background: linear-gradient(135deg, #667eea 0%, #764ba2 100%);\n  display: flex;\n  align-items: center;\n  justify-content: center;\n  font-size: 1.5rem;\n}\n\n/* Èí±ÂåÖ‰ø°ÊÅØÂå∫Âüü */\n.wallet-info-modern {\n  flex: 1;\n  text-align: left;\n  z-index: 10;\n  position: relative;\n}\n\n.wallet-title {\n  display: flex;\n  align-items: center;\n  gap: 0.375rem;\n  font-size: 1rem;\n  font-weight: 600;\n  color: #e5e5e5;\n  margin: 0 0 0.125rem 0;\n}\n\n.wallet-desc {\n  position: relative;\n  z-index: 10;\n  max-width: 10.625rem;\n  font-size: 0.75rem;\n  font-weight: 500;\n  color: #737373;\n  margin: 0;\n}\n\n/* Ë£ÖÈ•∞ÊÄßSVGËÉåÊôØ */\n.wallet-bg-svg {\n  position: absolute;\n  right: -1.25rem;\n  width: 13.75rem;\n  height: 3.375rem;\n  pointer-events: none;\n}\n\n@media (min-width: 768px) {\n  .wallet-bg-svg {\n    width: 18.75rem;\n  }\n}\n\n.wallet-connected {\n  text-align: center;\n}\n\n.connected-info {\n  background: #f8f9fa;\n  border-radius: 10px;\n  padding: 1.5rem;\n  margin: 1.5rem 0;\n}\n\n.wallet-address,\n.wallet-type {\n  display: flex;\n  justify-content: space-between;\n  margin-bottom: 0.5rem;\n  \n  &:last-child {\n    margin-bottom: 0;\n  }\n}\n\n.label {\n  color: #666;\n  font-weight: 500;\n}\n\n.address,\n.type {\n  color: #333;\n  font-family: monospace;\n}\n\n.disconnect-btn {\n  padding: 0.8rem 1.5rem;\n  background: #dc3545;\n  color: white;\n  border: none;\n  border-radius: 8px;\n  cursor: pointer;\n  transition: all 0.3s ease;\n}\n\n.disconnect-btn:hover {\n  background: #c82333;\n  transform: translateY(-1px);\n}\n\n.error-message {\n  margin-top: 1rem;\n  padding: 1rem;\n  background: #ffe6e6;\n  color: #cc0000;\n  border-radius: 8px;\n  text-align: center;\n}\n\n/* ËøûÊé•Áä∂ÊÄÅÂä®Áîª */\n.wallet-option-btn.connecting .wallet-icon-bg {\n  animation: pulse 1s infinite;\n}\n\n.wallet-option-btn.connecting {\n  border-color: #667eea;\n  background: #1e1b4b;\n}\n\n@keyframes pulse {\n  0% { opacity: 1; transform: scale(1); }\n  50% { opacity: 0.7; transform: scale(1.05); }\n  100% { opacity: 1; transform: scale(1); }\n}\n\n@media (max-width: 768px) {\n  .language-switcher {\n    top: 10px;\n    left: 10px;\n    \n    button {\n  